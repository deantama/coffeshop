public class Order {
    String name, type, sugar;
    int qty;

    public Order(String nama, String type, String sugar, int qty) {
        this.name = nama;
        this.type = type;
        this.sugar = sugar;
        this.qty = qty;
    }

    public String getName() {
        return name;
    }

    public String getType() {
        return type;
    }

    public String getSugar() {
        return sugar;
    }

    public int getQty() {
        return qty;
    }

}
